<?xml version="1.0"?>
<Container version="2">
  <Name>prometheus-qbittorrent-exporter</Name>
  <Repository>esanchezm/prometheus-qbittorrent-exporter</Repository>
  <Registry>https://hub.docker.com/r/esanchezm/prometheus-qbittorrent-exporter/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
